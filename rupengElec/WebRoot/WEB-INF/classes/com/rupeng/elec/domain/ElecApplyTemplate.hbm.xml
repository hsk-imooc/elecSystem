<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping>
    	<class name="com.rupeng.elec.domain.ElecApplyTemplate" table="elecApplyTemplate">
    		<!-- 配置唯一标识 -->
    		<id name="templateId" column="templateId">
    			<!-- 配置主键生成器 -->
    			<generator class="uuid"></generator>
    		</id>
    		
    		<!-- 配置普通属性 -->
    		<property name="filename" column="filename"></property>
    		<property name="path" column="path" ></property>
    		<property name="processDefinitionKey" column="processDefinitionKey"></property>
    	</class>
    </hibernate-mapping>